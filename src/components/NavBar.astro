---
import youtubeChannels from "../data/channels.json";
---

<nav class="menu-channels">
    <h1>Canales YouTube</h1>
    <ul>
        {youtubeChannels.map((channel) => (
            <li><a href="#" data-youtube={channel.id}>
            <img src={channel.logo} alt={channel.name} />
            {channel.name}</a>
            </li>
        ))}
    </ul>
</nav>
